{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^3.10|^4.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10|^11.0",
        "laravel/sanctum": "^3.3|^4.0",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "blackfire/php-sdk": "^2.5",
        "darkaonline/l5-swagger": "^9.0",
        "deptrac/deptrac": "^4.1",
        "enlightn/security-checker": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "infection/infection": "^0.31.2",
        "itsgoingd/clockwork": "^5.3",
        "larastan/larastan": "^2.11|^3.0",
        "laravel/dusk": "^8.3",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "laravel/telescope": "^5.11",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0|^8.0",
        "nunomaduro/phpinsights": "^2.13",
        "pestphp/pest": "^2.36|^3.0",
        "pestphp/pest-plugin-arch": "^3.1",
        "pestphp/pest-plugin-laravel": "^2.4|^3.0",
        "phploc/phploc": "^2.0",
        "phpmd/phpmd": "^2.15",
        "phpmetrics/phpmetrics": "^2.9",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.1",
        "roave/security-advisories": "dev-latest",
        "sentry/sentry-laravel": "^4.15",
        "spatie/laravel-ignition": "^2.0",
        "vimeo/psalm": "^6.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env' ) || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "php artisan test",
        "test:coverage": "php artisan test --coverage",
        "test:dusk": "php artisan dusk",
        "test:infection": "infection",
        "analyse": "php -d memory_limit=1G vendor/bin/phpstan analyse",
        "analyse:psalm": "psalm",
        "analyse:deptrac": "deptrac analyse",
        "analyse:phpmd": "phpmd app text phpmd.xml",
        "analyse:phploc": "phploc app",
        "analyse:phpinsights": "phpinsights analyse app",
        "analyse:security": "composer audit && enlightn security:check",
        "analyse:all": [
            "@analyse",
            "@analyse:psalm",
            "@analyse:deptrac",
            "@analyse:phpmd",
            "@analyse:phploc",
            "@analyse:phpinsights",
            "@analyse:security"
        ],
        "fix": "pint",
        "fix:rector": "rector process app --dry-run",
        "fix:rector:apply": "rector process app",
        "metrics": "phpmetrics app --config=phpmetrics.json",
        "quality": [
            "@fix",
            "@analyse:all",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
